{
  "name": "fitbit-api-handler",
  "version": "1.2.0",
  "description": "Unofficial handler for Fitbit API",
  "main": "./dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fabulator/fitbit-api-handler.git"
  },
  "bugs": {
    "url": "https://github.com/fabulator/fitbit-api-handler/issues"
  },
  "homepage": "https://github.com/fabulator/fitbit-api-handler",
  "author": "Michal Ozog√°n <michal@ozogan.eu>",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint ./src ./tests",
    "lint-export": "npm run lint -- -o ./tests_results/checkstyle/js-checkstyle.xml -f checkstyle",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report -f \"node node_modules/flow-bin/vendor/flow\" -i ./src/**/*.js* -t html -t text -t json -o tests_results/coverage/flow",
    "flow-init": "flow-typed install jest@22.x query-string@6.x luxon@0.4.x",
    "test": "jest",
    "test-build": "npm run test -- --config jest.config.build.js",
    "test-all": "npm run lint && npm run flow && npm run test",
    "build": "rm -rf ./dist && rollup --config ./rollup.config.js",
    "prepublishOnly": "npm run build && npm run test-build"
  },
  "devDependencies": {
    "@socifi/eslint-config": "^2.0.0",
    "flow-bin": "^0.84.0",
    "flow-coverage-report": "^0.6.0",
    "flow-typed": "^2.5.1",
    "@socifi/jest-config": "^1.10.0",
    "@socifi/rollup-config": "^1.10.0"
  },
  "dependencies": {
    "luxon": "^1.4.3",
    "mathjs": "^5.2.0",
    "query-string": "^6.2.0",
    "rest-api-handler": "^1.8.1"
  }
}
